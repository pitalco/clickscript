{
  "components": [
    {
      "name": "app-home",
      "props": [],
      "template": [
        {
          "element": "div",
          "attributes": [
            {
              "name": "class",
              "value": "app-home max-w-md mx-auto mt-10"
            }
          ],
          "children": [
            {
              "element": "h2",
              "attributes": [
                {
                  "name": "class",
                  "value": "text-2xl font-bold mb-6 text-gray-900 dark:text-white"
                }
              ],
              "content": "Welcome to Clickscript IDE"
            },
            {
              "element": "form",
              "children": [
                {
                  "element": "div",
                  "attributes": [
                    {
                      "name": "class",
                      "value": "mb-6"
                    }
                  ],
                  "children": [
                    {
                      "element": "label",
                      "attributes": [
                        {
                          "name": "for",
                          "value": "email"
                        },
                        {
                          "name": "class",
                          "value": "block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                        }
                      ],
                      "content": "Your email"
                    },
                    {
                      "element": "input",
                      "attributes": [
                        {
                          "name": "type",
                          "value": "email"
                        },
                        {
                          "name": "id",
                          "value": "email"
                        },
                        {
                          "name": "class",
                          "value": "bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"
                        },
                        {
                          "name": "placeholder",
                          "value": "name@example.com"
                        },
                        {
                          "name": "required",
                          "value": "true"
                        }
                      ]
                    }
                  ]
                },
                {
                  "element": "div",
                  "attributes": [
                    {
                      "name": "class",
                      "value": "mb-6"
                    }
                  ],
                  "children": [
                    {
                      "element": "label",
                      "attributes": [
                        {
                          "name": "for",
                          "value": "password"
                        },
                        {
                          "name": "class",
                          "value": "block mb-2 text-sm font-medium text-gray-900 dark:text-white"
                        }
                      ],
                      "content": "Your password"
                    },
                    {
                      "element": "input",
                      "attributes": [
                        {
                          "name": "type",
                          "value": "password"
                        },
                        {
                          "name": "id",
                          "value": "password"
                        },
                        {
                          "name": "class",
                          "value": "bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500"
                        },
                        {
                          "name": "required",
                          "value": "true"
                        }
                      ]
                    }
                  ]
                },
                {
                  "element": "button",
                  "attributes": [
                    {
                      "name": "type",
                      "value": "submit"
                    },
                    {
                      "name": "class",
                      "value": "text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 me-2 mb-2 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800"
                    }
                  ],
                  "content": "Submit"
                }
              ]
            }
          ]
        }
      ],
      "script": []
    }
  ],
  "server": [
    {
      "action": "log",
      "args": {
        "message": "Server started successfully",
        "level": "info"
      }
    },
    {
      "action": "log",
      "args": {
        "message": "Listening on port 3000",
        "level": "info"
      }
    }
  ]
}